<form name="form1" method="POST" action="/generate/websocket">
    <table>
        <tr>
            <td>Network name</td>
            <td><input type="text" name="network_name" ></td>
        </tr>
        <tr>
            <td>Network Password</td>
            <td><input type="text" name="network_password" ></td>
        </tr>
        <tr>
            <td>IP ADDRESS</td>
            <td><input type="text" value="192.168.0.155" name="network_ip" ></td>
        </tr>
        <tr>
            <td>Network gateway</td>
            <td><input type="text" value="192.168.0.1" name="network_gateway" ></td>
        </tr>
        <tr>
            <td>Network subnet</td>
            <td><input type="text" value="255.255.255.0" name="network_subnet" ></td>
        </tr>
        <tr>
            <td>Network dns</td>
            <td><input type="text" value="192.168.0.1" name="network_dns" ></td>
        </tr>
        <tr>
        <td colspan="2"><input type="submit" value="Generate Skeleton" ></td>
        </tr>
    </table>
</form>   